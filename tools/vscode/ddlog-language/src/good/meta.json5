{
  "repository": {
        "meta": {
            "patterns": [
                {
                    "name": "meta.attributes.ddlog",
                    "begin": "(#\\[)",
                    "end": "(\\])", 
                    "patterns": [  
                        {
                            "include": "#comment"
                        },
                        {
                            "include": "#constant"
                        },
                        {
                            "include": "#entity"
                        },
                        {
                            "include": "#meta"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#string"
                        },
                        {
                            "include": "#support"
                        },
                        {
                            "include": "#type"
                        },
                        {
                            "include": "#variable"
                        },
                        {
                            "include": "#operator"
                        },
                        {
                            "include": "#keyword"
                        },
                        {
                            "include": "#storage"
                        }
                    ]                                                         
                },
                {
                    "name": "meta.function-param.ddlog",
                    "begin": "(?<=(\\b)([a-z_][a-zA-Z0-9_]*))(\\()",
                    "end": "(\\))",
                    "beginCaptures": {"0": {"name": "punctuation.definition.type.ddlog"}},
                    "endCaptures": {"0": {"name": "punctuation.definition.type.ddlog"}},
                    "patterns": [
                        {
                            "match": "(\\b)([a-z_][a-zA-Z0-9_]*)(?=[(])",
                            "name": "entity.name.function.ddlog",
                            "captures": {
                            "1": {
                               "name": "punctuation.definition.type.ddlog"
                             }
                            }
                        },
                        {
                            "name": "variable.parameter.ddlog",
                            "match": "(?<![:]\\s*)(?!.*(bigint|bit|bool|signed|double|float|string|Vec|Ref|Set|Map))\\s*([a-z_][a-zA-Z0-9_]*)(\\b)"
                        },
                        {
                            "include": "#comment"
                        },
                        {
                            "include": "#constant"
                        },
                        {
                            "include": "#meta"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#string"
                        },
                        {
                            "include": "#support"
                        },
                        {
                            "include": "#type"
                        },
                        {
                            "include": "#variable"
                        },
                        {
                            "include": "#operator"
                        },
                        {
                            "include": "#keyword"
                        },
                        {
                            "include": "#storage"
                        }
                    ]
                }                                                               
            ]
        }
    }
}
